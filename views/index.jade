        li(data-target='#carousel-2', data-slide-to='2')
        li(data-target='#carousel-2', data-slide-to='3')
        li(data-target='#carousel-2', data-slide-to='4')
        li(data-target='#carousel-2', data-slide-to='5')
        li(data-target='#carousel-2', data-slide-to='6')
      .carousel-inner
        .item.active
          img(src='/images/IMG_3835_1.jpg', alt='...')
          .carousel-caption
            | Field 1
        .item
          img(src='/images/IMG_3841_1.jpg', alt='...')
          .carousel-caption
            | Field 2
        .item
          img(src='/images/IMG_3846_1.jpg', alt='...')
          .carousel-caption
            | Wall and Field
        .item
          img(src='/images/IMG_3850_1.jpg', alt='...')
          .carousel-caption
            | Sheep in Field
        .item
          img(src='/images/IMG_3867_1.jpg', alt='...')
          .carousel-caption
            | Path
        .item
          img(src='/images/IMG_3872_1.jpg', alt='...')
          .carousel-caption
            | Sheep 1
        .item
          img(src='/images/IMG_3873_1.jpg', alt='...')
